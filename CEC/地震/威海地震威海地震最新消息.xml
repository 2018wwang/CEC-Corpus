     <eRelation relType="Accompany" accompanya_eid="e12" accompanyb_eid="e13"/>
     <eRelation relType="Accompany" accompanya_eid="e14" accompanyb_eid="e15"/>
     <eRelation relType="Accompany" accompanya_eid="e15" accompanyb_eid="e16"/>
