<?xml version="1.0" encoding="GB2312"?>
<Body>    	<Title>合宁高速三车相撞 5死2伤</Title>
	<ReportTime type="absTime">2007-8-1</ReportTime>
	<Content>

    <Paragraph>
    	<Sentence>
    		<Event type="thoughtevent" eid="e1"><Participant sid="s1">新华网合肥</Participant><Time type="relTime" tid="t1">８月１日</Time><Denoter type="statement" did="d1">电</Denoter></Event>（记者程士华）
    		<Event eid="e2">记者从安徽省合肥市交警部门获悉，
    			<Time type="relTime" tid="t2">７月３１日凌晨</Time>，
    			<Location lid="l2">合宁高速３５公里处</Location>发生一起
    			<Object oid="o2">三辆车</Object>连环
    			<Denoter type="action" did="d2">相撞</Denoter>的特大交通事故
    		</Event>，造成
    		<Event eid="e3">
    			<Participant oid="o3">５人</Participant>
    			<Denoter type="stateChange" did="d3">死亡</Denoter>
    			</Event>，
    		<Event eid="e4">
    			<Participant oid="o4">２人</Participant>
    			<Denoter type="stateChange" did="d4">受伤</Denoter>
    		</Event>。
    	</Sentence>
    	<Sentence>
    		<Event eid="e5">
    			<Time type="relTime" tid="t5">目前</Time>
    			<Object oid="o5">事故现场</Object>已
    			<Denoter type="operation" did="d5">清理</Denoter>完毕
    		</Event>，
    		<Event eid="e6">
    			<Object oid="o6">交通</Object>
    			<Denoter type="stateChange" did="d6">恢复畅通</Denoter>
    		</Event>。
    	</Sentence>
    </Paragraph>
	<Paragraph>
		<Sentence>据
			<Event eid="e7">
				<Participant sid="s7">高速交警支队二大队</Participant>
				<Denoter type="statement" did="d7">介绍</Denoter>
			</Event>，
			<Event eid="e8">
				<Time type="relTime" tid="t8">３１日凌晨</Time>，
				<Object oid="o8">两辆货车</Object>一前一后，
				<Denoter type="action" did="d8">停靠</Denoter>
				<Location lid="l8">在紧急停车道上</Location>
			</Event>，
			<Event eid="e9">后面的
				<Object  sid="s9">一辆货车</Object>准备
				<Denoter type="action" did="d9">更换</Denoter>
				<Object oid="o9">车轮胎</Object>
			</Event>。
			<Event eid="e10">
				<Object sid="s10">一辆油罐车</Object>
				<Denoter type="movement" did="d10">行至</Denoter>
				<Location type="destination" lid="l10">事发地点处</Location>
			</Event>，
			<Event eid="e11">猛烈地
				<Denoter type="action" did="d11">撞上</Denoter>了正在更换车胎的
				<Object oid="o11">货车</Object>
			</Event>。
		</Sentence>
		<Sentence>
			<Event eid="e12">
				<Object oid="o12">前面的一辆货车</Object>也受到了强大的
				<Denoter type="action" did="d12">冲击</Denoter>
			</Event>，
			<Event eid="e13">
				<Denoter type="action" did="d13">撞上</Denoter>了高速公路的
				<Object oid="o13">隔离栏</Object>
			</Event>。
		</Sentence>
		<Sentence>
			<Event eid="e14">
				<Object oid="o14">三车</Object>连环
				<Denoter type="action" did="d14">相撞</Denoter>
			</Event>导致
			<Event eid="e15">当场有
				<Participant oid="o15">４人</Participant>
				<Denoter type="stateChange" did="d15">死亡</Denoter>
			</Event>，
			<Event eid="e16">
				<Participant oid="o16">１人</Participant>在送往医院的途中
				<Denoter type="stateChange" did="d16">死亡</Denoter>
			</Event>，
			<Event eid="e17">另有
				<Participant oid="o17">２人</Participant>
				<Denoter type="stateChange" did="d17">受伤</Denoter>
			</Event>。
		</Sentence>
		</Paragraph>
		<Paragraph>
			<Sentence>目前２名伤者伤势稳定，
				<Event eid="e18">
					<Object oid="o18">事故现场</Object>已
					<Denoter type="operation" did="d18">清理</Denoter>完毕
				</Event>，
				<Event eid="e19">
					<Object oid="o19">交通</Object>
					<Denoter type="stateChange" did="d19">恢复畅通</Denoter>
				</Event>。
				<Event eid="e20">
					<Object oid="o20">事故原因</Object>正在进一步
					<Denoter type="operation" did="d20">调查</Denoter>中
				</Event>。
			</Sentence>
		</Paragraph>
	</Content>
<eRelation relType="Causal" cause_eid="e2" effect_eid="e3"/>
<eRelation relType="Causal" cause_eid="e2" effect_eid="e4"/> 
